{
  "name": "custom-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "all"
        ],
        "path": "./all.json"
      },
      {
        "language": [
          "taskini"
        ],
        "path": "./taskini.json"
      },
      {
        "language": [
          "sh"
        ],
        "path": "./sh.json"
      },
      {
        "language": [
          "c",
          "cpp"
        ],
        "path": "./cpp.json"
      },
      {
        "language": "go",
        "path": "./go.json"
      },
      {
        "language": "python",
        "path": "./python.json"
      },
      {
        "language": "dockerfile",
        "path": "./dockerfile.json"
      },
      {
        "language": [
          "gitcommit",
          "NeogitCommitMessage"
        ],
        "path": "./gitcommit.json"
      },
      {
        "language": "vim",
        "path": "./vim.json"
      }
    ]
  }
}
